{
    "name": "Intecmedia.Webpack",
    "version": "0.0.5",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "scripts": {
        "clean": "cross-env NODE_ENV=production node script.clean.js",
        "update": "cross-env NODE_ENV=production node script.update.js",
        "development": "cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\"--inspect\" webpack --mode=development --profile --progress=profile",
        "debug": "cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\"--inspect\" webpack --mode=development --profile --progress=profile",
        "preproduction": "npm run app-lint",
        "production": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --profile --progress=profile",
        "preprod": "npm run pre-build",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --profile --progress=profile",
        "prod-debug": "cross-env NODE_ENV=production DEBUG=1 NODE_OPTIONS=\"--inspect\" webpack --mode=production --profile --progress=profile",
        "watch": "npm run watch-dev",
        "watch-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=development\"",
        "watch-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=development\"",
        "watch-prod": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=production DEBUG=0 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=production\"",
        "js-lint": "cross-env NODE_ENV=production eslint --config .eslintrc.js --ignore-path .eslintignore --fix --no-error-on-unmatched-pattern *.js source/**/*.js --cache --cache-location ./node_modules/.cache/eslint-js/",
        "js-lint-config": "cross-env NODE_ENV=production eslint --print-config .eslintrc.js --ignore-path .eslintignore",
        "ts-lint": "cross-env NODE_ENV=production eslint --config .eslintrc.typescript.js --ignore-path .eslintignore --fix --no-error-on-unmatched-pattern source/**/*.ts --cache --cache-location ./node_modules/.cache/eslint-ts/",
        "ts-lint-config": "cross-env NODE_ENV=production eslint --print-config .eslintrc.typescript.js --ignore-path .eslintignore",
        "css-lint": "cross-env NODE_ENV=production stylelint source/**/*.scss --fix --syntax scss --ignore-path .stylelintignore --config .stylelintrc.js --cache --cache-location ./node_modules/.cache/stylelint/",
        "css-lint-config": "cross-env NODE_ENV=production stylelint --syntax scss  --ignore-path .stylelintignore --print-config .stylelintrc.js",
        "html-validate": "cross-env NODE_ENV=production node ./script.html-validate.js --path-suffix",
        "html-validate-dev": "cross-env NODE_ENV=development node ./script.html-validate.js --path-suffix",
        "html-validator": "cross-env NODE_ENV=production node ./script.html-validator.js --path-suffix",
        "app-lint": "cross-env NODE_ENV=production node ./script.app-lint.js",
        "image-lint": "cross-env NODE_ENV=production node ./script.image-lint.js --path-suffix",
        "lint": "npm run app-lint && npm run js-lint && npm run ts-lint && npm run css-lint && npm run html-validate && npm run html-validator && npm run image-lint",
        "server": "npm run server-dev",
        "preserver-dev": "npm run clean",
        "server-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\\\"--inspect --max-old-space-size=4096\\\" webpack serve --mode=development\"",
        "preserver-debug": "npm run clean",
        "server-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\\\"--inspect --max-old-space-size=4096\\\" webpack serve --mode=development\"",
        "pre-build": "npm run app-lint && npm run js-lint && npm run css-lint && npm run utf-fix && npm run svgo && npm run image-lint && npm run fonts-subsets",
        "post-build": "npm run html-validate && npm run html-validator",
        "prebuild": "npm run pre-build",
        "postbuild": "npm run post-build",
        "build": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --profile --progress=profile",
        "browserslist-dev": "cross-env NODE_ENV=development npx browserslist",
        "browserslist-prod": "cross-env NODE_ENV=production npx browserslist",
        "browserslist": "cross-env NODE_ENV=production npx browserslist",
        "fonts-subsets": "cross-env NODE_ENV=production node ./script.fonts-subsets.js",
        "utf-fix": "cross-env NODE_ENV=production node ./script.utf-fix.js",
        "svgo": "cross-env NODE_ENV=production node ./script.svgo.js",
        "start": "npm run build"
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": {
        "production": [
            "iOS >= 12",
            "Safari >= 12",
            "Chrome >= 60",
            "ChromeAndroid >= 60",
            "Opera >= 60",
            "Firefox >= 60",
            "Firefox ESR"
        ],
        "development": [
            "last 1 Safari versions",
            "last 1 Chrome versions",
            "last 1 Firefox versions"
        ]
    },
    "engines": {
        "fonttools": "*.*.*",
        "node": ">=16",
        "npm": ">=7"
    },
    "dependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/eslint-plugin": "^7.14.5",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-proposal-decorators": "^7.16.0",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
        "@babel/plugin-proposal-optional-chaining": "^7.16.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.16.0",
        "@babel/plugin-syntax-top-level-await": "^7.14.5",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@gfx/zopfli": "^1.0.15",
        "@popperjs/core": "^2.10.2",
        "@sentry/browser": "^6.14.3",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "autoprefixer": "^10.4.0",
        "babel-loader": "^8.2.3",
        "bootstrap": "^5.1.3",
        "browserslist": "^4.18.1",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "compression-webpack-plugin": "^9.0.0",
        "copy-webpack-plugin": "^9.1.0",
        "core-js": "^3.19.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "cssnano": "^5.0.10",
        "date-fns": "^2.25.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-compat": "^3.13.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-webpack-plugin": "^3.1.1",
        "expose-loader": "^3.1.0",
        "fastparse": "^1.1.2",
        "favicons-webpack-plugin": "^5.0.2",
        "file-loader": "^6.2.0",
        "find-cache-dir": "^3.3.2",
        "focus-visible": "^5.2.0",
        "focus-within": "^3.0.2",
        "front-matter": "^4.0.2",
        "glob": "^7.2.0",
        "html-validate": "^6.1.2",
        "html-validator": "^5.1.17",
        "html-webpack-plugin": "^5.5.0",
        "ignore": "^5.1.9",
        "image-minimizer-webpack-plugin": "^2.2.0",
        "image-size": "^1.0.0",
        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^9.0.0",
        "imagemin-webp": "^6.0.0",
        "imports-loader": "^3.1.1",
        "intersection-observer": "^0.12.0",
        "jquery": "^3.6.0",
        "js-beautify": "^1.14.0",
        "line-column": "^1.0.2",
        "loader-utils": "^3.2.0",
        "lodash.merge": "^4.6.2",
        "lodash.template": "^4.5.0",
        "lodash.uniqueid": "^4.0.1",
        "md5-file": "^5.0.0",
        "mini-css-extract-plugin": "^2.4.4",
        "nodemon": "^2.0.15",
        "nunjucks": "^3.2.3",
        "p-limit": "^3.1.0",
        "postcss": "^8.3.11",
        "postcss-bem-linter": "^3.3.0",
        "postcss-browser-reporter": "^0.6.0",
        "postcss-devtools": "^1.1.1",
        "postcss-fail-on-warn": "^0.2.1",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-focus": "^5.0.1",
        "postcss-focus-visible": "^6.0.1",
        "postcss-focus-within": "^5.0.1",
        "postcss-font-display": "^0.3.0",
        "postcss-loader": "^6.2.0",
        "postcss-reporter": "^7.0.4",
        "postcss-url": "^10.1.3",
        "resize-observer-polyfill": "^1.5.1",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "schema-utils": "^3.1.1",
        "semver": "^7.3.5",
        "sharp": "0.28.3",
        "slash": "^3.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-config-twbs-bootstrap": "^2.2.4",
        "stylelint-csstree-validator": "^1.9.0",
        "stylelint-high-performance-animation": "^1.6.0",
        "stylelint-max-lines": "^0.1.0",
        "stylelint-max-root-rules": "^0.1.0",
        "stylelint-no-nested-media": "^0.1.0",
        "stylelint-no-unsupported-browser-features": "^5.0.2",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.21.0",
        "stylelint-selector-bem-pattern": "^2.1.1",
        "stylelint-use-nesting": "^3.0.0",
        "stylelint-webpack-plugin": "^3.1.0",
        "svg-spritemap-webpack-plugin": "^4.3.3",
        "svgo": "^2.8.0",
        "terser-webpack-plugin": "^5.2.5",
        "typescript": "^4.4.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.64.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0",
        "webpack-log": "^3.0.2",
        "webpack-sources": "^3.2.2",
        "whatwg-fetch": "^3.6.2",
        "yargs": "^17.2.1"
    }
}
