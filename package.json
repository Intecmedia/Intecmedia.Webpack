{
    "name": "Intecmedia.Webpack",
    "version": "0.0.5",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "type": "commonjs",
    "scripts": {
        "clean": "cross-env NODE_ENV=production node script.clean.js",
        "clear-cache": "npx rimraf ./node_modules/.cache",
        "update": "cross-env NODE_ENV=production node script.update.js",
        "update-package": "cross-env NODE_ENV=production node ./script.update-package.js",
        "update-package-hash": "cross-env NODE_ENV=production node ./script.update-package.js --hash-only",
        "development": "cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\"--inspect\" webpack --mode=development --progress=profile",
        "debug": "cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\"--inspect-brk\" webpack --mode=development --progress=profile",
        "preproduction": "npm run app-lint && npm run update-package-hash",
        "production": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --progress=profile",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --progress=profile",
        "prod-debug": "cross-env NODE_ENV=production DEBUG=1 NODE_OPTIONS=\"--inspect-brk\" webpack --mode=production --progress=profile",
        "watch": "npm run watch-dev",
        "watch-dev": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=development\"",
        "watch-debug": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=development\"",
        "watch-prod": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=production DEBUG=0 NODE_OPTIONS=\\\"--max-old-space-size=4096\\\" webpack --watch --mode=production\"",
        "js-lint-cmd": "cross-env NODE_ENV=production eslint --config .eslintrc.js --no-error-on-unmatched-pattern --fix --cache --cache-location ./node_modules/.cache/eslint/",
        "js-lint-cmd-dev": "cross-env NODE_ENV=development eslint --config .eslintrc.js --no-error-on-unmatched-pattern --fix --cache --cache-location ./node_modules/.cache/eslint/",
        "js-lint": "npm run js-lint-cmd *.js source/**/*.js",
        "js-lint-dev": "npm run js-lint-cmd-dev *.js source/**/*.js",
        "js-lint-config": "cross-env NODE_ENV=production eslint --config .eslintrc.js --print-config source/js/app.js",
        "js-lint-config-dev": "cross-env NODE_ENV=development eslint --config .eslintrc.js --print-config source/js/app.js",
        "js-lint-config-node": "cross-env NODE_ENV=production eslint --config .eslintrc.js --print-config webpack.config.js",
        "css-lint-cmd": "cross-env NODE_ENV=production stylelint --fix --config .stylelintrc.js --ignore-path .stylelintignore --allow-empty-input --cache --cache-location ./node_modules/.cache/stylelint/",
        "css-lint": "npm run css-lint-cmd source/**/*.scss",
        "css-lint-config": "cross-env NODE_ENV=production stylelint --ignore-path .stylelintignore --config .stylelintrc.js --print-config source/css/app.css",
        "css-lint-config-dev": "cross-env NODE_ENV=development stylelint --ignore-path .stylelintignore --config .stylelintrc.js --print-config source/css/app.css",
        "html-validate": "cross-env NODE_ENV=production node ./script.html-validate.js",
        "html-validate-prod": "cross-env NODE_ENV=production node ./script.html-validate.js",
        "html-validate-dev": "cross-env NODE_ENV=development node ./script.html-validate.js",
        "html-validator": "cross-env NODE_ENV=production node ./script.html-validator.js",
        "html-beautify": "cross-env NODE_ENV=production node ./script.html-beautify.js",
        "html-typograf": "cross-env NODE_ENV=production node ./script.html-typograf.js",
        "app-lint": "cross-env NODE_ENV=production node ./script.app-lint.js",
        "image-lint": "cross-env NODE_ENV=production node ./script.image-lint.js",
        "lint": "npm run app-lint && npm run js-lint && npm run css-lint && npm run html-validate && npm run html-validator && npm run image-lint",
        "server": "npm run server-dev --",
        "server-https": "npm run server-dev-https --",
        "preserver-dev": "npm run clean",
        "server-dev": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\\\"--inspect --max-old-space-size=4096\\\" webpack serve --mode=development\"",
        "preserver-dev-https": "npm run clean",
        "server-dev-https": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=0 NODE_OPTIONS=\\\"--inspect --max-old-space-size=4096\\\" webpack serve --https --mode=development\"",
        "preserver-debug": "npm run clean",
        "server-debug": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\\\"--inspect-brk --max-old-space-size=4096\\\" webpack serve --mode=development\"",
        "preserver-debug-https": "npm run clean",
        "server-debug-https": "node ./script.nodemon.js --exec \"npx cross-env NODE_ENV=development DEBUG=1 NODE_OPTIONS=\\\"--inspect-brk --max-old-space-size=4096\\\" webpack serve --https --mode=development\"",
        "prebuild": "npm run app-lint && npm run filename-lint && npm run js-lint && npm run css-lint && npm run utf-fix && npm run svgo && npm run image-lint && npm run fonts-subsets && npm run html-beautify",
        "postbuild": "npm run html-validate && npm run html-validator",
        "build": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production --progress=profile",
        "browserslist-dev": "cross-env NODE_ENV=development npx browserslist",
        "browserslist-prod": "cross-env NODE_ENV=production npx browserslist",
        "browserslist": "cross-env NODE_ENV=production npx browserslist",
        "fonts-subsets": "cross-env NODE_ENV=production node ./script.fonts-subsets.js",
        "utf-fix": "cross-env NODE_ENV=production node ./script.utf-fix.js",
        "svgo": "cross-env NODE_ENV=production node ./script.svgo.js",
        "filename-lint": "cross-env NODE_ENV=production node ./script.filename-lint.js",
        "start": "npm run server --",
        "prettier": "cross-env NODE_ENV=production prettier --config ./.prettierrc.js --cache --write",
        "prettier-js": "cross-env NODE_ENV=production prettier --config ./.prettierrc.js --cache --write *.js source/**/*.js",
        "prettier-css": "cross-env NODE_ENV=production prettier --config ./.prettierrc.js --cache --write source/**/*.scss",
        "prettier-check-js": "cross-env NODE_ENV=production eslint-config-prettier source/js/app.js webpack.config.js",
        "lint-staged": "npx lint-staged --relative --allow-empty --no-stash --concurrent false --verbose",
        "husky-install": "npx husky",
        "pre-commit-hook": "npm run lint-staged -- --quiet",
        "post-merge-hook": "npm run update-package",
        "prepare": "npm run update-package-hash && npm run husky-install"
    },
    "lint-staged": {
        "source/**/*!(.min).js": [
            "npm run filename-lint",
            "npm run prettier",
            "npm run js-lint-cmd"
        ],
        "source/**/*.scss": [
            "npm run filename-lint",
            "npm run prettier",
            "npm run css-lint-cmd"
        ],
        "source/**/*.html": [
            "npm run filename-lint",
            "npm run html-beautify"
        ],
        "source/**/*.{jpg,jpeg,png,svg,gif}": [
            "npm run filename-lint",
            "npm run image-lint"
        ],
        "source/**/*.svg": [
            "npm run svgo"
        ]
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": {
        "production": [
            "iOS >= 14",
            "Safari >= 14",
            "Chrome >= 90",
            "ChromeAndroid >= 90",
            "Opera >= 90",
            "Firefox >= 90",
            "Firefox ESR"
        ],
        "development": [
            "last 2 Safari major versions",
            "last 2 Chrome versions",
            "last 2 Firefox versions"
        ]
    },
    "engines": {
        "@pip/fonttools": "*.*.*",
        "node": ">=22",
        "npm": ">=11"
    },
    "overrides": {
        "sharp": "^0.34.5"
    },
    "dependencies": {
        "@babel/cli": "^7.28.6",
        "@babel/core": "^7.29.0",
        "@babel/eslint-parser": "^7.28.6",
        "@babel/eslint-plugin": "^7.27.1",
        "@babel/plugin-proposal-decorators": "^7.29.0",
        "@babel/plugin-proposal-optional-chaining-assign": "^7.27.1",
        "@babel/plugin-proposal-pipeline-operator": "^7.28.6",
        "@babel/plugin-syntax-top-level-await": "^7.14.5",
        "@babel/plugin-transform-class-properties": "^7.28.6",
        "@babel/plugin-transform-logical-assignment-operators": "^7.28.6",
        "@babel/plugin-transform-nullish-coalescing-operator": "^7.28.6",
        "@babel/plugin-transform-optional-chaining": "^7.28.6",
        "@babel/preset-env": "^7.29.0",
        "@babel/preset-react": "^7.28.5",
        "@babel/preset-typescript": "^7.28.5",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
        "@popperjs/core": "^2.11.8",
        "@sentry/browser": "^10.38.0",
        "@sentry/tracing": "^7.120.4",
        "autoprefixer": "^10.4.24",
        "babel-loader": "^10.0.0",
        "bootstrap": "^5.3.8",
        "browserslist": "^4.28.1",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "compression-webpack-plugin": "^11.1.0",
        "copy-webpack-plugin": "^13.0.1",
        "core-js": "^3.48.0",
        "cross-env": "^10.1.0",
        "css-loader": "^7.1.3",
        "cssnano": "^7.1.2",
        "date-fns": "^4.1.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-compat": "^6.1.0",
        "eslint-plugin-jsdoc": "^62.5.4",
        "eslint-plugin-n": "^17.23.2",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-webpack-plugin": "^5.0.2",
        "fastparse": "^1.1.2",
        "favicons": "^7.2.0",
        "favicons-webpack-plugin": "^6.0.1",
        "file-loader": "^6.2.0",
        "focus-visible": "^5.2.1",
        "front-matter": "^4.0.2",
        "glob": "^13.0.1",
        "globals": "^17.3.0",
        "gsap": "^3.14.2",
        "html-validate": "^10.7.0",
        "html-validator": "^6.0.1",
        "html-webpack-plugin": "^5.6.6",
        "husky": "^9.1.7",
        "ignore": "^7.0.5",
        "image-minimizer-webpack-plugin": "^4.1.4",
        "image-size": "^2.0.2",
        "js-beautify": "^1.15.4",
        "js-cookie": "^3.0.5",
        "line-column": "^1.0.2",
        "lint-staged": "^16.2.7",
        "loader-utils": "^3.3.1",
        "lodash.debounce": "^4.0.8",
        "lodash.merge": "^4.6.2",
        "lodash.range": "^3.2.0",
        "lodash.template": "^4.5.0",
        "lodash.throttle": "^4.1.1",
        "micromatch": "^4.0.8",
        "mini-css-extract-plugin": "^2.10.0",
        "nodemon": "^3.1.11",
        "nunjucks": "^3.2.4",
        "p-limit": "^7.3.0",
        "postcss": "^8.5.6",
        "postcss-browser-reporter": "^0.7.0",
        "postcss-fail-on-warn": "^0.2.1",
        "postcss-focus": "^7.0.0",
        "postcss-focus-visible": "^11.0.0",
        "postcss-font-display": "^0.3.0",
        "postcss-loader": "^8.2.0",
        "postcss-reporter": "^7.1.0",
        "postcss-scss": "^4.0.9",
        "postcss-sort-media-queries": "^5.2.0",
        "postcss-url": "^10.1.3",
        "prettier": "^3.8.1",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "^6.1.2",
        "sass-embedded": "^1.97.3",
        "sass-loader": "^16.0.7",
        "schema-utils": "^4.3.3",
        "semver": "^7.7.4",
        "sharp": "^0.34.5",
        "stylelint": "^16.26.1",
        "stylelint-config-recommended-scss": "^17.0.0",
        "stylelint-config-standard-scss": "^16.0.0",
        "stylelint-high-performance-animation": "^1.11.0",
        "stylelint-no-nested-media": "^0.1.0",
        "stylelint-no-unsupported-browser-features": "^8.1.1",
        "stylelint-order": "^7.0.1",
        "stylelint-prettier": "^5.0.3",
        "stylelint-scss": "^6.14.0",
        "stylelint-selector-bem-pattern": "^4.0.1",
        "stylelint-semantic-groups": "^1.2.2",
        "stylelint-webpack-plugin": "^5.1.0",
        "svg-spritemap-webpack-plugin": "^5.1.0",
        "svgo": "^4.0.0",
        "terser-webpack-plugin": "^5.3.16",
        "typescript": "^5.9.3",
        "typograf": "^7.6.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.105.0",
        "webpack-assets-manifest": "^6.5.0",
        "webpack-bundle-analyzer": "^5.2.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.3",
        "webpack-log": "^3.0.2",
        "webpack-sources": "^3.3.3",
        "yargs": "^18.0.0"
    }
}
