{
    "name": "Intecmedia.Webpack",
    "version": "0.0.3",
    "description": "Intecmedia Webpack bootstrap",
    "repository": {
        "type": "git",
        "url": "https://github.com/Intecmedia/Intecmedia.Webpack.git"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack -p",
        "watch": "npm run watch-dev",
        "watch-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 webpack --watch -d\"",
        "watch-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 webpack --watch -d\"",
        "js-lint": "cross-env NODE_ENV=development eslint --config .eslintrc --ignore-path .eslintignore --fix *.js source/**/*.js",
        "js-lint-config": "cross-env NODE_ENV=development eslint --print-config .eslintrc --ignore-path .eslintignore",
        "css-lint": "cross-env NODE_ENV=development stylelint source/**/*.scss --fix --syntax scss --formatter verbose --ignore-path .stylelintignore --config .stylelintrc",
        "css-lint-config": "cross-env NODE_ENV=development stylelint --syntax scss  --ignore-path .stylelintignore --print-config .stylelintrc",
        "html-lint": "cross-env NODE_ENV=development node ./script.html-lint.js",
        "html-validator": "cross-env NODE_ENV=development node ./script.html-validator.js",
        "app-lint": "cross-env NODE_ENV=development node ./script.app-lint.js",
        "lint": "npm run app-lint && npm run js-lint && npm run css-lint && npm run html-lint && npm run html-validator",
        "server": "npm run server-dev",
        "server-dev": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=0 webpack-dev-server --hot --inline -d\"",
        "server-debug": "node ./script.nodemon.js --exec \"cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline -d\"",
        "build": "npm-run-all app-lint js-lint css-lint prod html-lint html-validator",
        "browserslist-dev": "cross-env NODE_ENV=development npx browserslist",
        "browserslist-prod": "cross-env NODE_ENV=production npx browserslist",
        "browserslist": "cross-env NODE_ENV=production npx browserslist",
        "fonts-subsets": "cross-env NODE_ENV=development node ./script.fonts-subsets.js",
        "start": "npm run build"
    },
    "author": "Dmitry Pyatkov",
    "license": "MIT",
    "browserslist": {
        "production": [
            "Android >= 5",
            "IE >= 10",
            "iOS >= 9",
            "Safari >= 9",
            "last 2 Edge versions",
            "last 2 Chrome versions",
            "last 2 ChromeAndroid version",
            "last 2 Firefox versions",
            "last 2 Opera versions"
        ],
        "development": [
            "last 1 Safari versions",
            "last 1 Chrome versions",
            "last 1 Firefox versions",
            "last 1 Opera versions"
        ]
    },
